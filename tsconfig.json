{
    "compilerOptions": {
      "baseUrl": ".",
      "strict": true,
      "rootDir": ".",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "target": "es2016",
      "module": "esnext",
      "noImplicitAny": false,
      "removeComments": true,
      "preserveConstEnums": true,
      "downlevelIteration": true,
      "lib": ["esnext", "DOM"],
      "paths": {
        "@toy-vue3/*": ["src"]
      }
    },
    "include": ["src"]
  }
  